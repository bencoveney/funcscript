{
  "name": "funcscript",
  "version": "0.1.0",
  "description": "Functional programming helpers for TypeScript and JavaScript.",
  "main": "bin/index.js",
  "scripts": {
    "build": "tsc",
    "check": "node --experimental-modules bin/test.js",
    "docs": "typedoc --exclude \"**/*+(test|index).ts\" --out docs/ src/ && nodetouch docs/.nojekyll",
    "test": "npm run build && npm run check && npm run unit-test && npm run docs",
    "unit-test": "jasmine --config=jasmine.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bencoveney/funcscript.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bencoveney/funcscript/issues"
  },
  "homepage": "https://github.com/bencoveney/funcscript#readme",
  "dependencies": {
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "jasmine": "^3.1.0",
    "touch": "^3.1.0",
    "typedoc": "^0.11.1"
  }
}
